#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        int n,i,j,a[502][502],f,c=0,of=0;
        cin>>n;
        for(i=0;i<n;i++)
        {
            string s;
            cin>>s;
            for(j=0;j<n;j++)
                a[i][j]=s[j]-'0';
        }
        while(1)
        {f=0;of=0;
            for(i=0;i<n;i++)
            {
                for(j=0;j<n;j++)
                {
                    if(a[i][j]==1)
                        of=1;
                    if(a[i][j]==2)
                    {f=1;a[i][j]=0;
                        if(i-1>=0 )
                        {
                            if(a[i-1][j]==1)
                                a[i-1][j]=3;
                        }
                        if(i+1<n )
                        {
                            if(a[i+1][j]==1)
                                a[i+1][j]=3;
                        }
                        if(j+1<n)
                        {
                            if(a[i][j+1]==1)
                                a[i][j+1]=3;
                        }
                        if( j-1>=0)
                        {
                            if(a[i][j-1]==1)
                                a[i][j-1]=3;
                        }
                    }
                }
            }if(f==0)
                break;
          for(i=0;i<n;i++)
            {
                for(j=0;j<n;j++)
                {if(a[i][j]==3)
                 a[i][j]=2;}}
            c++;
        }
        if(of==1)
            cout<<"-1\n";
        else
        cout<<c-1<<"\n";
    }
    return 0;
}
