#include <iostream>
using namespace std;
struct node
{
    int data;
    node* left;
    node* right;
};
node*  create(int data1)
{
    node* newnode= new node();
    newnode->data=data1;
    newnode->left=newnode->right=NULL;
    return newnode;
}
node* insert(node* root,int data1)
{
    if(root==NULL)
    {
        root=create(data1);
    }
    else if(root->data>=data1)
    root->left=insert(root->left,data1);
    else
    root->right=insert(root->right,data1);
    
    return root;
}
node* lca(node* root,int a,int b)
{
    if(root==NULL)
        return NULL;
if(root->data>max(a,b))
    return lca(root->left,a,b);
    if(root->data<min(a,b))
        return lca(root->right,a,b);
        else
            return root;
}

int main() {
    // your code goes here
    int t;
    cin>>t;
    while(t--){
    int n,a,k,b;
    node* root=NULL;
    cin>>n>>k;
    while(n--)
    {
        cin>>a;
       root= insert(root,a);
 
    
    }
        while(k--)
        {
            cin>>a>>b;
           node* x=lca(root,a,b);
            if(x!=NULL)
            cout<<x->data<<" ";
        }
cout<<"\n";
    }
    return 0;
}
