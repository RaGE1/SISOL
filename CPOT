#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    unsigned long long int t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        if(!(n&n-1))cout<<"True"<<endl;
        else cout<<"False"<<endl;
    }    
    return 0;
}
