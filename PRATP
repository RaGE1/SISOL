//https://www.hackerrank.com/contests/smart-interviews/challenges/si-print-right-angled-triangle-pattern
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t,n,z=0;
    cin>>t;
    while(t--)
    {
        cin>>n;
        cout<<"Case #"<<++z<<":"<<endl;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(i+j+1>=n)cout<<"*";
                else cout<<" ";
            }
            cout<<endl;
        }
        
    }
    return 0;
}
