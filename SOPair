#include<bits/stdc++.h>
using namespace std;
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);cout.precision(10);
    int t;
    cin>>t;
    while(t--)
    {
        unordered_map <int,int> mp;
        int n,k;
        cin>>n>>k;
        int a[n];
        for(int i = 0;i<n;i++)
        {
            cin>>a[i];
            mp[a[i]]++;
        }
        bool flag = false;
        for(int i=0;i<n;i++)
        {   mp[a[i]]--;
            if(mp[k-a[i]]!=0)
                {
                    flag = true;
                    break;
                }
            mp[a[i]]++;
        }
        if(flag)
            cout<<"True"<<endl;
        else
            cout<<"False"<<endl;
        
    }
    return 0;
}
