#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        long long int n;
        cin>>n;
        if(n%6)
            cout<<"Santa\n";
        else
            cout<<"Banta\n";
    }
    return 0;
}
