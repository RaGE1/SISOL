#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

long long int mod=1000000007;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
     long long    int n,m,b,a[1000][1000],i,j,k,l,co=0,dp[1000][1000];
        cin>>n>>m>>b;
        for(i=0;i<n;i++)
            for(j=0;j<m;j++)
            {dp[i][j]=0; a[i][j]=0;}
        while(b--)
        {
            cin>>k>>l;
            a[k][l]=1;
        
        }
        for(i=0;i<n;i++)
            if(a[i][0]!=1)
                dp[i][0]=1;
        else 
            break;
        for(i=0;i<m;i++)
            if(a[0][i]!=1)
                dp[0][i]=1;
        else 
            break;
        
        
        if(a[0][0]==0 && a[n-1][m-1]==0)
        {
           for(i=1;i<n;i++)
           {
               for(j=1;j<m;j++)
                   if(a[i][j]!=1)
                   dp[i][j]=(dp[i-1][j]+dp[i][j-1]+dp[i-1][j-1])%1000000007;
            
           }
        }
        cout<<dp[n-1][m-1]<<"\n";
    }
    return 0;
}
