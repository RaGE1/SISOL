#include<bits/stdc++.h>
using namespace std;
int main (){
    int t; 
    int a[200];
    cin>>t;
    while(t--){
     string y;
     cin>>y;
     for(int i=97;i<=122;i++) a[i]=0;
     int len=y.length();
     for(int i=0;i<len;i++) 
     a[(int)y[i]]++;
     int ans=0;
     for(int i=97;i<=122;i++)
        ans=ans^a[i];
     if(ans!=0) 
     cout<<"Santa\n";
     else 
         cout<<"Banta\n";
    }
      
  return 0;
}
