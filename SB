#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int a[32]={0};
        for(int i=31;i>=0;i--)
        {
            if(n&(1<<i))a[i] = 1;
        }
        for(int i=0;i<31;i+=2)
        {
            if(a[i]^a[i+1])
            {
                int temp = a[i];
                a[i] = a[i+1];
                a[i+1] = temp;
            }
        }
        n = 0;
        for(int i=31;i>=0;i--)
        {
            if(a[i])
                n = n^(1<<i);
        }
        cout<<n<<endl;
    }
    return 0;
}
