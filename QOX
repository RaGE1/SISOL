#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

int t;
   scanf("%d",&t);
    while(t--)
    {
        int n,c=0,a1[10000],a2[10000],a3[10000],a4[10000],i,j,fre1[100000],fre2[100000];
        scanf("%d",&n);
        for(i=0;i<n;i++)
            scanf("%d",&a1[i]);
           for(i=0;i<n;i++)
            scanf("%d",&a2[i]);
           for(i=0;i<n;i++)
            scanf("%d",&a3[i]);
           for(i=0;i<n;i++)
            scanf("%d",&a4[i]);
        for(i=0;i<128;i++)
        {fre1[i]=0;fre2[i]=0;}
        for(i=0;i<n;i++)
        {
            for(j=0;j<n;j++)
            {
            fre1[(a1[i]^a2[j])]++;
              
                fre2[a3[i]^a4[j]]++;
               
            }
        } 
            for(i=0;i<128;i++)
            {
             
                    c=c+(fre1[i]*fre2[i]);
            }
     printf("%d\n",c);
    }
}
