#include <iostream>
using namespace std;
struct node
{
    int data;
    node* left;
    node* right;
};
node*  create(int data1)
{
    node* newnode= new node();
    newnode->data=data1;
    newnode->left=newnode->right=NULL;
    return newnode;
}
node* insert(node* root,int data1)
{
    if(root==NULL)
    {
        root=create(data1);
    }
    else if(root->data>=data1){
    root->left=insert(root->left,data1);}
    else{
    root->right=insert(root->right,data1);
        }
    return root;
}
int isfb(node* root)
{if(root==NULL)
    return 1;
    if(root->left==NULL && root->right!=NULL)
        return 0;
    if(root->left!=NULL && root->right==NULL)
        return 0;
    if(isfb(root->left)==1 && isfb(root->right)==1)
        return 1;
 else
     return 0;
}
int main() {
    // your code goes here
    int t;
    cin>>t;
    while(t--){
    int n,a;
    node* root=NULL;
    cin>>n;
    while(n--)
    {
        cin>>a;
       root= insert(root,a);
       
    }
  if(isfb(root))
    cout<<"True\n";
    else
        cout<<"False\n";
    }
    return 0;
}
