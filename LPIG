#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


vector<int>v[100000];
int vis[100000];
int x,mdep,ans;

void dfs(int node,int dep)
{
    vis[node]=1;
    if(mdep<dep)
    {
        mdep=dep,
        x=node;
    }
    for(int i=0;i<(int)v[node].size();i++)
    {
        if(!vis[v[node][i]])
            dfs(v[node][i],dep+1);
    }
}
int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int t;
    cin>>t;
    while(t--)
    {
        int n,m,i;
        cin>>n>>m;
        for(i=0;i<=n;i++)
            v[i].clear(),vis[i]=0;
        
         int a,b;
        while(m--)
            cin>>a>>b,
            v[a].push_back(b),
            v[b].push_back(a);
        mdep=-1;
        dfs(1,0);ans=-1;
        for(i=0;i<=n;i++)
        {
            vis[i]=0;
        }
        dfs(x,0);
        cout<<mdep<<"\n";
        
    }
    return 0;
}
