#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        int x,s=0;
        int s1=((n+1)*(n+2))/2;
        for(int i=0;i<n;i++)
        {
            cin>>x;
            s+=x;
        }
        cout<<s1-s<<endl;
    }
    return 0;
}
