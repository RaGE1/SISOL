#include <bits/stdc++.h>

using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        int n,k;
        cin>>n>>k;
    int a[1000000];
        for(int i=0;i<n;i++)
            cin>>a[i];
      
      map<int,int>m;
        int i;
   for( i=0;i<k;i++)
       m[a[i]]++;
        cout<<m.size()<<" ";
        i=0;
        for( ;k<n;k++)
        { 
            m[a[i]]--;
            if(m[a[i]]==0)
                m.erase(a[i]);
            m[a[k]]++;i++;
          
        
            cout<<m.size()<<" ";
        }
        cout<<"\n";
    }
    return 0;
}
